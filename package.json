  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint src tests server",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "better-npm-run test",
    "deploy": "better-npm-run deploy",
    "build:dev": "better-npm-run deploy:devserver",
    "build:intstg": "better-npm-run deploy:intstg",
    "build:stg": "better-npm-run deploy:stg",
    "postinstall": "node nightwatch.conf.js",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "sauce": "./node_modules/.bin/nightwatch -e chrome",
    "e2e-setup": "/node_modules/selenium-standalone/.bin/selenium-standalone.cmd install",
    "tests": "jest",
    "test:end": "node_modules/.bin/nightwatch",
    "testy": "./node_modules/.bin/nightwatch"
  },
  "betterScripts": {
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/dev-server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:devserver": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "devserver",
        "DEBUG": "app:*"
      }
    },
    "deploy:intstg": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "intstg",
        "DEBUG": "app:*"
      }
    },
    "deploy:stg": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "stg",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
